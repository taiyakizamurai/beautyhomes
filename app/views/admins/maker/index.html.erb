
  <table class="table table-hover table-inverse">
    <thead>
      <tr>
        <th><h1>メーカー一覧</h1></th>
        <th><%= link_to "➕", new_admins_maker_path, class: "btn btn" %></th>
      </tr>
      <tr>
        <th><p>メーカーID</p></th>
        <th><p>メーカー名</p></th>


      </tr>
    </thead>

    <tbody>
      <% @makers.each do |maker| %>
         <tr>
           <td><%= maker.id %></td>
           <td><%= link_to maker.name, admin_maker_path(maker.id) %></td>
         </tr>
       <% end %>
    </tbody>
  </table>